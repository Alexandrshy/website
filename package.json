{
  "name": "parcel-website",
  "version": "1.0.0",
  "description": "The parcel website",
  "main": "index.js",
  "repository": "git://git@github.com:parcel-bundler/website.git",
  "author": "Devon Govett <devongovett@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "rm -rf dist",
